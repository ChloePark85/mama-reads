{
  "manifest_version": 2,
  "name": "TTS Web Reader",
  "version": "1.0",
  "description": "Reads web pages using company TTS API",
  "permissions": ["activeTab", "http://61.250.94.154/"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
